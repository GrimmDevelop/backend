{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.0.0",
        "ext-redis": ">=5.0",
        "cviebrock/laravel-elasticsearch": "^8.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "flowjs/flow-php-server": "^1.0",
        "fruitcake/laravel-cors": "^2.0",
        "hisamu/php-xbase": "^1.0",
        "illuminate/redis": "^v8.0",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "phpoffice/phpspreadsheet": "^1.2",
        "pusher/pusher-php-server": "^4.0",
        "spatie/laravel-backup": "^7.0",
        "spatie/laravel-fractal": "^5.0",
        "spatie/laravel-medialibrary": "^8.7.1",
        "spatie/valuestore": "^1.0"
    },
    "require-dev": {
        "laravel/tinker": "^2.0",
        "beyondcode/laravel-dump-server": "^1.4",
        "barryvdh/laravel-ide-helper": "^2.0",
        "filp/whoops": "~2.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Grimm\\": "entities/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
