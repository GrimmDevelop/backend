{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4.0",
        "barryvdh/laravel-ide-helper": "^2.0",
        "cviebrock/laravel-elasticsearch": "^4.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "flowjs/flow-php-server": "^1.0",
        "hisamu/php-xbase": "^1.0",
        "illuminate/redis": "^6.0",
        "laravel/framework": "^6.0",
        "laravel/passport": "^8.0",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "phpoffice/phpspreadsheet": "^1.2",
        "predis/predis": "^1.0",
        "pusher/pusher-php-server": "^4.0",
        "smalot/pdfparser": "^0.14",
        "spatie/laravel-backup": "^6.0",
        "spatie/laravel-fractal": "^5.0",
        "spatie/laravel-medialibrary": "^7.0",
        "spatie/valuestore": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.3",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Grimm\\": "entities/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
